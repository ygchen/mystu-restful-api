<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="edu.stu.user.bean.UserCover" table="stu_user_cover">
        <id name="id" type="long">
            <column name="id" length="16" />
            <generator class="identity" />
        </id>
        <property name="userId" type="int">
            <column name="user_id" precision="11" scale="0" />
        </property>
        <property name="url" type="string">
            <column name="url" length="255" not-null="true" />
        </property>
       <property name="createdAt" type="long">
            <column name="created_at" precision="16" scale="0" />
        </property> 
    </class>
</hibernate-mapping>
